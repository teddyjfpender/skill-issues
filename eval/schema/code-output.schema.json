{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Codex Code Output",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "code": {
      "type": "string",
      "description": "Generated source code."
    },
    "notes": {
      "type": "string",
      "description": "Optional reasoning or caveats (kept out of the code file)."
    }
  },
  "required": ["code", "notes"]
}
